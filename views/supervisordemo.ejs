<!DOCTYPE html>
<html>
<head>
	<title> Supervisor Demo </title>
	<% include layout/scripts.ejs %>
</head>

<body>
<% include layout/header.ejs %>

<section>
	<div> 
		<% if (newsArray) { %>
			<% for (i=0; i<newsArray.length; i++) { %>
				<div>
					<div>Title: <%- newsArray[i].title %> </div>
					<div>Description: <%- newsArray[i].description %> </div>
					<div>Link: <%- newsArray[i].link %> </div>
					<br />
				</div>
			<% } %>
		<% } %>
	</div>
</section>

<% include layout/footer.ejs %>
</body>
</html>